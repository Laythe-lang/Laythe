assert(nil == nil); // expect: true

assert(true == true); // expect: true
assertEq(true == false, false); // expect: false

assert(1 == 1); // expect: true
assertEq(1 == 2, false); // expect: false

assert("str" == "str"); // expect: true
assertEq("str" == "ing", false); // expect: false

assertEq(nil == false, false); // expect: false
assertEq(false == 0, false); // expect: false
assertEq(0 == "0", false); // expect: false
