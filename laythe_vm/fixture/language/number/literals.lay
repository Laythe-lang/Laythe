assertEq(123, 100 + 23);     // expect: 123
assertEq(987654, 987650 + 4);  // expect: 987654
assertEq(0, 2 -2);       // expect: 0
assertEq(-0, 0 * -1);      // expect: -0

assertEq(123.456, 123456 / 1000); // expect: 123.456
assertEq(-0.001, 0 - 0.001);  // expect: -0.001
assertEq(3e3, 3000);
assertEq(9e+0, 9);
assertEq(1.4e-2, 0.014);
