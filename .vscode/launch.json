  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'spacelox_vm'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--lib",
          "--package=spacelox_vm"
        ],
        "filter": {
          "name": "spacelox_vm",
          "kind": "lib"
        }
      },
      "args": [],
      "sourceLanguages": ["rust"],
      "cwd": "${workspaceFolder}/spacelox_vm"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "spacelox repl",
      "cargo": {
        "args": [
          "build",
          "--bin=spacelox",
          "--package=spacelox"
        ],
        "filter": {
          "name": "spacelox",
          "kind": "bin"
        }
      },
      "args": [],
      "sourceLanguages": ["rust"],
      "cwd": "${workspaceFolder}/spacelox"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "spacelox script",
      "cargo": {
        "args": [
          "build",
          "--bin=spacelox",
          "--package=spacelox"
        ],
        "filter": {
          "name": "spacelox",
          "kind": "bin"
        }
      },
      "args": [
        "assert/assert_eq.lox"
      ],
      "sourceLanguages": ["rust"],
      "cwd": "${workspaceFolder}/spacelox_vm/fixture"
    }
  ]
}